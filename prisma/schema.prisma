generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL") // uses connection pooling
  directUrl = env("POSTGRES_URL_NON_POOLING") // uses a direct connection
}


model User {
  id String @id @default(uuid())
  email String @unique
  password String
}

model Movie {
  id String @id @default(uuid())
  title String
  publishing_year Int
  cover String
}